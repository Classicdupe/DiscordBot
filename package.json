{
    "name": "dscbot",
    "version": "1.1.0",
    "description": "The discord bot for ClassicDupe",
    "main": "index.js",
    "scripts": {
        "dev": "tsc --watch",
        "lint": "eslint --ext .js,.ts .",
        "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
        "build": "tsc",
        "start": "cd build && node ./index.js",
        "containerBuild": "docker build -t ghcr.io/classicdupe/discordbot:1.1.0 .",
        "containerPush": "docker push ghcr.io/classicdupe/discordbot:1.1.0",
        "container": "npm run containerBuild && npm run containerPush"
    },
    "keywords": [],
    "author": "Prorickey",
    "license": "ISC",
    "dependencies": {
        "discord.js": "^14.11.0",
        "dotenv": "^16.0.3",
        "mariadb": "^3.1.2",
        "ws": "^8.13.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.41.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-standard-with-typescript": "^36.0.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-n": "^15.7.0",
        "eslint-plugin-promise": "^6.1.1",
        "prettier": "^3.0.0",
        "typescript": "^5.0.4"
    }
}
